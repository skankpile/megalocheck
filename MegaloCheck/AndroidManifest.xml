<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="jm.megalocheck"
    android:versionCode="101"
    android:versionName="1.1" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/mc_icon"
        android:label="@string/app_name"
        android:theme="@style/AppBaseTheme" >
        <activity
            android:name="megalocheck.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="megalocheck.Screen2_Activity"
            android:label="@string/title_activity_screen2_"
            android:parentActivityName="megalocheck.MainActivity"
            android:windowSoftInputMode="stateHidden" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="megalocheck.MainActivity" />
        </activity>
        <activity
            android:name="megalocheck.MixScreen"
            android:label="@string/title_activity_mix_screen"
            android:parentActivityName="megalocheck.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="megalocheck.MainActivity" />
        </activity>
        <activity
            android:name="megalocheck.SysScreen"
            android:label="@string/title_activity_sys_screen"
            android:parentActivityName="megalocheck.MainActivity"
            android:windowSoftInputMode="stateHidden" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="megalocheck.MainActivity" />
        </activity>
         <activity
            android:name="megalocheck.prefs"
            android:label="@string/title_prefs_sys_screen"
            android:parentActivityName="megalocheck.MainActivity"
            android:windowSoftInputMode="stateHidden" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="megalocheck.MainActivity" />
        </activity>
    </application>

</manifest>
